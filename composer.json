{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "artesaos/seotools": "^0.23.0",
        "blade-ui-kit/blade-heroicons": "^1.3",
        "calebporzio/sushi": "^2.4",
        "guzzlehttp/guzzle": "^7.4",
        "laravel/framework": "^9.37.0",
        "laravel/sanctum": "^2.14.1",
        "laravel/telescope": "^4.9",
        "laravel/tinker": "^2.7",
        "milon/barcode": "^9.0",
        "rappasoft/laravel-livewire-tables": "^2.7",
        "simplesoftwareio/simple-qrcode": "~2",
        "spatie/laravel-analytics": "^4.1",
        "stripe/stripe-php": "^9.5",
        "wire-elements/modal": "^1.0",
        "wireui/wireui": "^1.12"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "laravel-lang/attributes": "^2.0",
        "laravel-lang/lang": "^12.0",
        "laravel-lang/publisher": "^14.3",
        "laravel/breeze": "^1.10",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "rector/rector": "^0.13.7",
        "spatie/laravel-ignition": "^1.0",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
